
config droidnet 'device'
	option id ''
	option display_app '10'
	option display_msg '10'

config droidnet 'monitoring'
	option enable '1'
	option ping 'http'
	option host 'google.com'
	option success_limit '10'
	option failure_limit '10'
	option wait_time '5'
	option interface 'wan1'
	option restart '0'
	option tunnel_service 'tunnel'

